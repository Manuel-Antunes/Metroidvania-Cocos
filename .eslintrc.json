{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "globals": {
    "cc": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "class-methods-use-this": "warn",
    "no-underscore-dangle": "off",
    "prettier/prettier": [
      "error"
    ]
  }
}
